@model IEnumerable<Category>

<!DOCTYPE html>

<html lang="en">
<head>
    <title>Index Category</title>
</head>
<body>
<div>
    <table class="table table-bordered table-striped" style="width: 100%">
        <thead>
        <tr>
            <th>Category Name</th>
            <th>Display Name</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="50%">@obj.Name</td>
                <td width="30%">@obj.DisplayOrder</td>
            </tr>
        }
        </tbody>
    </table> 
</div>
</body>
</html>